<script>
    ;(function(){
var nav = {
    prev:$(".prev"),
    next:$(".next"),
    box:$(".content_inner"),
    isrun:0,
    init: function(){
        this.minw = $(".introduce").offset().left+$(".introduce").width();
        this.prev.on("click",function(){
            nav.cn="prev";
            nav.setBox();
            return false;
        });
        this.next.on("click",function(){
            nav.cn="next";
             nav.setBox();
             return false;
        });
        $(".content_inner").add(this.next).add(this.prev).on({
            "mouseenter": function(){
                $("body").addClass("show_nav");
            },
            "mouseleave": function(){
                $("body").removeClass("show_nav");
            }
        });
        this.setBtn();
    },
    setBtn: function(){
        var w = parseInt($(document).width());
        if(w<this.minw){
            if(!this.isrun){
                this.next.addClass("active");
                this.isrun=1;
            }
            this.setBox(this.cn);
        }else{
            this.isrun=0;
            this.cn="prev";
            this.box.css("left",0);
            this.next.add(this.prev).removeClass("active");
        }
    },
    setBox: function(an){
        var w = parseInt($(document).width());
        this.box.animate({
            "left": this.cn==="next" ? -(this.minw+30-w) : 0
        },function(){
            if(nav.cn==="next"){
                nav.prev.addClass("active");
                nav.next.removeClass("active");
            }else{
                nav.prev.removeClass("active");
                nav.next.addClass("active");
            }
        });
    }
};

setTimeout(function(){
    nav.init();
},500);

var m_timer;

$(window).on("resize",function(){
    clearTimeout(m_timer);
    m_timer =setTimeout(function(){
        nav.setBtn();
    },100);
});

})();
</script>
